<table class="table text-white">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Mail</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{name}}</td>
            <td>{{age}}</td>
            <td>{{mail}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
<h1>Logout</h1>
<button><a style="color: white;" href="/logout">Logout</a></button>